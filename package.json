{
  "name": "@examples/trpc-next-prisma-starter",
  "version": "10.7.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "run-s dev:next",
    "dev:next": "next dev",
    "build": "run-s generate:prisma db:migrate build:next",
    "build:next": "next build",
    "db:migrate": "prisma migrate deploy",
    "generate:prisma": "prisma generate",
    "start": "run-s build start:next",
    "start:next": "next start",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:typecheck": "tsc --pretty --noEmit",
    "lint:prettier": "prettier --check .",
    "format": "run-s format:*",
    "format:eslint": "yarn lint:next --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.3.8",
    "@trpc/client": "^10.7.0",
    "@trpc/next": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "clsx": "^1.1.1",
    "framer-motion": "^6",
    "next": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
